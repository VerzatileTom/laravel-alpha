{
  "name": "wanmigs/laravel-alpha",
  "description": "Laravel Auth Preset Package",
  "type": "package",
  "require": {
    "php": "^7.2",
    "laravel/passport": "^8.0",
    "spatie/laravel-permission": "^3.6.0"
  },
  "license": "mit",
  "authors": [
    {
      "name": "Juan Miguel Torres",
      "email": "wanmigs@gmail.com",
      "homepage": "https://github.com/wanmigs/laravel-alpha"
    }
  ],
  "autoload": {
    "psr-4": {
      "Fligno\\Auth\\": "src/"
    }
